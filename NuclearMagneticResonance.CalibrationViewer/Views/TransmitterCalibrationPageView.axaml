<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:NuclearMagneticResonance.CalibrationViewer.ViewModels"
             xmlns:ScottPlot="clr-namespace:ScottPlot.Avalonia;assembly=ScottPlot.Avalonia"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="NuclearMagneticResonance.CalibrationViewer.Views.TransmitterCalibrationPageView">
  <Design.DataContext>
    <vm:TransmitterCalibrationPageViewModel/>
  </Design.DataContext>

  <ScrollViewer HorizontalScrollBarVisibility="Visible">
    <StackPanel Spacing="5"
                Margin="11">
      <TextBlock Text="Калибровка передатчика"
                   HorizontalAlignment="Center"
                   FontSize="22"
                 Margin="0 0 0 20">
      </TextBlock>
      </StackPanel>
    </ScrollViewer>
  
</UserControl>
